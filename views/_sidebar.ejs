<nav id="sidebar">
    <% if(!(typeof(sidebar) === 'undefined' || sidebar == [])) { %>
    <ul>
        <% sidebar.forEach(function(menu) {
        var name = menu.name
          , id = name.replace(/\s+/, '');
        %>
        <li id="<%= id %>">
            <a href="#<%= id %>"><%= name %></a>
            <ul>
                <% menu.items.forEach(function(item) { %>
                <li><a href="<%= item.link %>"><%= item.name %></a></li>
                <% }); %>
            </ul>
        </li>
        <% }); %>
    </ul>
    <% } %>
    <ul>
        <li id="impressum">
            <a href="#impressum">Impressum</a>
            <ul>
                <li><a href="#impressum">Impressum</a></li>
                <li><a href="#impressum">Kontakt</a></li>
            </ul>
        </li>
    </ul>
</nav>